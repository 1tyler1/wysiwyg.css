{
  "name": "wysiwyg.css",
  "version": "0.0.1",
  "description": "A tiny CSS for generated HTML or Markdown content",
  "main": "wysiwyg.css",
  "scripts": {
    "autoprefix": "postcss --use autoprefixer --output wysiwyg.css wysiwyg.css",
    "build": "npm run clean && npm run build-wysiwyg && npm run autoprefix",
    "build-docs": "node-sass --output-style compressed docs/docs.sass docs/docs.css",
    "build-wysiwyg": "node-sass --output-style expanded wysiwyg.sass wysiwyg.css",
    "clean": "rm -rf wysiwyg.css",
    "start": "npm run build-docs -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jgthms/wysiwyg.css.git"
  },
  "keywords": [
    "css",
    "markdown",
    "sass",
    "generated",
    "html"
  ],
  "author": "Jeremy Thomas <bbxdesign@gmail.com> (http://jgthms.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jgthms/wysiwyg.css/issues"
  },
  "homepage": "https://github.com/jgthms/wysiwyg.css#readme",
  "dependencies": {
    "autoprefixer": "^6.4.1",
    "minireset.css": "0.0.2",
    "node-sass": "^3.9.3",
    "postcss-cli": "^2.6.0"
  }
}
